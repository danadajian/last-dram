{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-native",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleDetection": "force",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext"
  }
}
