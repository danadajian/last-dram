{
  "main": "expo-router/entry",
  "packageManager": "bun@1.2.3",
  "scripts": {
    "build:docker": "docker build . --tag last-dram && docker-compose up",
    "build:ios": "eas build --platform ios",
    "build:web": "expo export --platform web",
    "cy:component": "NODE_ENV=production cypress open --component --browser chrome",
    "dev:web": "bun --hot src/server.ts & bun start:web",
    "dev:ios": "bun --hot src/server.ts & bun start:ios",
    "format": "prettier --write .",
    "start:ios": "NODE_ENV=development expo start --ios",
    "start:web": "NODE_ENV=development expo start --web",
    "start": "bun src/server.ts",
    "submit:ios": "eas submit --platform ios --latest"
  },
  "dependencies": {
    "@clerk/clerk-expo": "2.7.8",
    "@expo/metro-runtime": "4.0.1",
    "@trpc/client": "10.45.2",
    "@trpc/react-query": "10.45.2",
    "@trpc/server": "10.45.2",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-native": "0.73.0",
    "eas-cli": "15.0.12",
    "expo": "52.0.37",
    "expo-router": "4.0.17",
    "expo-secure-store": "14.0.1",
    "expo-status-bar": "2.0.1",
    "expo-updates": "0.27.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-web": "0.19.13",
    "trpc-bun-adapter": "1.2.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "10.0.3",
    "@types/testing-library__cypress": "5.0.13",
    "bun-types": "1.2.4",
    "cypress": "14.0.3",
    "husky": "9.1.7",
    "prettier": "3.5.2",
    "vite": "6.2.0",
    "typescript": "5.7.3"
  }
}
