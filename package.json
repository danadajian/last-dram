{
  "main": "expo-router/entry",
  "packageManager": "bun@1.2.4",
  "scripts": {
    "build:docker": "docker build . --tag last-dram && docker-compose up",
    "build:ios": "eas build --platform ios",
    "build:web": "expo export --platform web && bun scripts/replace-html-paths.ts",
    "cy:component": "cypress open --component --browser chrome",
    "dev:backend": "bun --hot src/backend/server.ts",
    "dev:web": "API_URL=http://localhost:3000 expo start --web --clear",
    "dev:ios": "API_URL=http://localhost:3000 expo start --ios --clear",
    "format": "prettier --write .",
    "start:backend": "bun src/backend/server.ts",
    "submit:ios": "eas submit --platform ios --latest"
  },
  "dependencies": {
    "@clerk/clerk-expo": "2.13.1",
    "@expo/metro-runtime": "4.0.1",
    "@tanstack/react-query": "5.80.7",
    "@trpc/client": "11.4.1",
    "@trpc/react-query": "11.4.1",
    "@trpc/server": "11.4.1",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "bun-types": "1.2.16",
    "drizzle-orm": "0.44.2",
    "drizzle-kit": "0.31.1",
    "eas-cli": "16.10.1",
    "expo": "52.0.46",
    "expo-router": "4.0.21",
    "expo-secure-store": "14.2.3",
    "expo-status-bar": "2.2.3",
    "expo-updates": "0.28.14",
    "pg": "8.16.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.80.0",
    "react-native-safe-area-context": "4.14.1",
    "react-native-web": "0.20.0",
    "trpc-bun-adapter": "1.3.0",
    "typescript": "5.8.3",
    "valibot": "1.1.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "10.0.3",
    "@types/testing-library__cypress": "5.0.13",
    "cypress": "14.4.1",
    "husky": "9.1.7",
    "prettier": "3.5.3",
    "vite": "6.3.5"
  }
}
