{
  "main": "src/main.ts",
  "packageManager": "bun@1.2.3",
  "scripts": {
    "build:docker": "docker build . --tag last-dram && docker-compose up",
    "build:ios": "eas build --platform ios",
    "build:web": "expo export:web",
    "cy:component": "NODE_ENV=production cypress open --component --browser chrome",
    "dev:web": "bun --hot src/server.ts & bun start:web",
    "dev:ios": "bun --hot src/server.ts & bun start:ios",
    "format": "prettier --write .",
    "start:ios": "NODE_ENV=development EXPO_NO_TYPESCRIPT_SETUP=1 expo start --ios",
    "start:web": "NODE_ENV=development EXPO_NO_TYPESCRIPT_SETUP=1 expo start --web",
    "start": "bun src/server.ts",
    "submit:ios": "eas submit --platform ios --latest"
  },
  "dependencies": {
    "@clerk/clerk-expo": "0.19.25",
    "@expo/webpack-config": "19.0.0",
    "@trpc/client": "10.44.1",
    "@trpc/react-query": "10.44.1",
    "@trpc/server": "10.44.1",
    "@types/cors": "2.8.17",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.0.10",
    "@types/react-native": "0.72.8",
    "babel-loader": "9.1.3",
    "babel-preset-react-app": "10.0.1",
    "cors": "2.8.5",
    "eas-cli": "5.9.2",
    "expo": "49.0.21",
    "expo-secure-store": "12.3.1",
    "expo-status-bar": "1.6.0",
    "expo-updates": "0.18.19",
    "express": "4.18.2",
    "express-rate-limit": "7.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-safe-area-context": "4.6.3",
    "react-native-web": "0.19.9",
    "webpack": "5.89.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@testing-library/cypress": "10.0.3",
    "@types/testing-library__cypress": "5.0.13",
    "cypress": "14.0.3",
    "husky": "9.1.7",
    "prettier": "3.5.2",
    "typescript": "5.7.3"
  }
}
